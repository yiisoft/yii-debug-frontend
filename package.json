{
  "name": "yii-debug-frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ci:build": "ng build --prod",
    "ci:test": "ng test --karma-config=src/karma.ci.js --browsers=ChromeHeadlessNoSandbox --code-coverage=true --watch=false",
    "ci:lint": "eslint -f checkstyle ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.7",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~11.0.7",
    "@angular/compiler": "~11.0.7",
    "@angular/core": "~11.0.7",
    "@angular/forms": "~11.0.7",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~11.0.7",
    "@angular/platform-browser-dynamic": "~11.0.7",
    "@angular/router": "~11.0.7",
    "core-js": "^3.6.5",
    "rxjs": "~6.6.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.6",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@angular/cli": "^11.0.6",
    "@angular/compiler-cli": "~11.0.7",
    "@angular/language-service": "~11.0.7",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~14.0.22",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "typescript": "~4.0.5"
  }
}
